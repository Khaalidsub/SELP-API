{
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
    
  ],
  "exclude": [
    "node_modules",
    "**/*.d.ts",
    "**/index.ts",
    "**/interfaces/**",
    "**/*.spec.ts"
  ],
  "reporter": [
    "text-summary",
    "html",
    "lcov"
  ],
  "extension": [
    ".ts"
  ],
  "check-coverage": true,
  "lines": 50,
  "statements": 50,
  "functions": 5,
  "branches": 50,
  "all": true
}
